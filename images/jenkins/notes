FROM tomcat:8.0-jre8
MAINTAINER PPK
ADD "http://ftp-chi.osuosl.org/pub/jenkins/war-stable/2.121.1/jenkins.war" "/usr/local/tomcat/webapps/jenkins.war"
EXPOSE 8080 #application runs on 8080 port
CMD ["catalina.sh","run"] #

docker run -it tomcat:8-jre8 /bin/bash
docker build -t pk-jenkins:1.0 .
docker run -it -p 80:8080 -d pk-jenkins:1.0

